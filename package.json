{
  "name": "unit-tests-examples",
  "version": "1.0.0",
  "description": "Unit test examples project",
  "scripts": {
    "start": "nodemon --watch src --exec ts-node src/index.ts",
    "test": "jest --bail --silent --noStackTrace --detectOpenHandles --logHeapUsage --runInBand --passWithNoTests",
    "build": "rm -rf ./dist && node_modules/.bin/tsc --project tsconfig.build.json --preserveSymlinks"
  },
  "keywords": [
    "unit tests",
    "hapi",
    "jest",
    "sqlite3"
  ],
  "author": "Yuri Matin",
  "license": "ISC",
  "dependencies": {
    "@hapi/hapi": "20.1.0",
    "joi": "17.4.0",
    "sqlite3": "5.0.1",
    "uuid": "3.4.0"
  },
  "devDependencies": {
    "@types/hapi__hapi": "20.0.5",
    "@types/jest": "26.0.20",
    "@types/sqlite3": "3.1.7",
    "@types/uuid": "8.3.0",
    "jest": "26.6.3",
    "nodemon": "2.0.7",
    "prettier": "2.2.1",
    "ts-jest": "26.5.1",
    "ts-node": "9.1.1",
    "typescript": "4.1.3"
  },
  "jest": {
    "preset": "ts-jest"
  }
}
